    "@typescript-eslint/parser": "8.7.0",
    "@types/node": "22.7.4",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "prettier": "3.3.3",
    "turbo": "2.1.1",
    "typescript": "5.6.2"
  }
}
